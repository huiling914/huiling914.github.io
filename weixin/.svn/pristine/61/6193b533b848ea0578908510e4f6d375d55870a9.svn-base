.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.cover {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
}
.block{
  /**width: 90%;
  margin-left: 5%;*/
  background-color: #ffffff;
  border-radius: 10px;
  margin-top: 16px;
  box-shadow: 2px 2px 4px rgba(131, 131, 131, 0.2);
}
.banner{
  width: 100%;
  height: 40px;
  display: flex;
  flex-direction: row;
}
.banner-line{
  margin: 13px 13px;
  margin-left: 18px;
  width: 4px;
  height: 14px;
  background-color: #25aab1;
}
.banner text{
  font-size: 14px;
  color: #466466;
  line-height: 40px;
}
.invalid{
  border: 1rpx #da2b2b solid;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
button[disabled][type="default"], wx-button[disabled]:not([type]) {
  background-color: transparent !important;
  border: none !important;
}
.chat{
  border: none;
  background-color: transparent;
  width: 70rpx;
  height: 70rpx;
  text-align: center;
  position: fixed;
  bottom: 25%;
  left: 10rpx;
  padding: 0;
  margin: 0;
  z-index: 9999;
}
.chat::after{
  border: none;
}
.icon-chat , .icon-chatchat{
  color: #25aab1;
  font-size: 65rpx;
  line-height: 70rpx;
}