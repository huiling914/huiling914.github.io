page{
  height:100%;
  background-color: #fff;
}
.vol-con{

font-size:16px;
height:100%
}
.vol-rec{
  text-align: right;
  color:#466466;
   border-bottom:1px #eee solid;
   padding:5px;
}
.single-list{
  border-bottom:1px #eee solid;
  padding:10px
}
.distance{
  color:#a2a2a2;
  font-size:12px;
  height:10px;
  float:right;
  position:relative;
  bottom:25px;
}
.title{
  display: inline-block;
  width:100%;
}
.label{
  color:#a2a2a2;
  height:30px;
}
.schoolname{
  color:#466466;
}
.sign-btn{
  width:50px;
  height:30px;
  background-color:#25aab1;
  color:#fff;
  line-height:30px;
  text-align: center;
  border-radius: 5px;
  position:relative;
  margin-top:-30px;
  float:right;
}
text{
  position:relative;
  margin-top:-55px;
  float:right;
  color:#a2a2a2;
}
.no-data {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.icon-no-record, .icon-notfound {
  color: #466466;
  font-size: 70px;
  line-height: 90px;
}

.no-data-text {
  color: #466466;
  font-size: 16px;
  line-height: 20px;
}
