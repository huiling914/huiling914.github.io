import request from '@/utils/request'
//上传店铺 logo图
export function uploadBrandIcon(data) {
  return request({
    url: 'http://www.lebao108.com:3020/res/file/upload/uploadBrandIcon',
    method: 'post',
    headers: {
            //'X-Requested-With': 'XMLHttpRequest',
            'Content-Type': 'multipart/form-data; charset=UTF-8'
          },
    data
  })
}
//删除店铺logo
export function branddelFile(data) {
  return request({
    url: 'http://www.lebao108.com:3020/res/file/brand/delFile',
    method: 'post',
    data
  })
}
//上传商品图片
export function uploadGoodsDetail(data) {
  return request({
    url: 'http://www.lebao108.com:3020/res/file/upload/uploadGoodsDetail',
    method: 'post',
