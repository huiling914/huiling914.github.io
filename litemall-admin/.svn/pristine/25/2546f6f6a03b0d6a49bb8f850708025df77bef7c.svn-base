import request from '@/utils/request'

export function listSchool(query) {
  return request({
    url: 'http://www.lebao108.com:3020/mall/admin/school/list',
    method: 'get',
    params: query
  })
}
export function AddSchool(data) {
  return request({
    url: 'http://www.lebao108.com:3020/mall/admin/school/create',
    method: 'post',
    data
  })
}
export function UpdateSchool(data) {
  return request({
    url: 'http://www.lebao108.com:3020/mall/admin/school/update',
    method: 'post',
    data
  })
}
export function DeleteSchool(query) {
  return request({
    url: 'hhttp://www.lebao108.com:3020/mall/admin/school/delete',
    method: 'get',
    params:query
  })
}
