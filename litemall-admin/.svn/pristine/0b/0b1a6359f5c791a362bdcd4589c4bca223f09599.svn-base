import request from '@/utils/request'

export function listQuestion(query) {
  return request({
    url: '/base/user/question/listWeb',
    method: 'get',
    params: query
  })
}
export function addQuestion(data){
	return request({
		url:'/base/user/question/add',
		method:'post',
		data
	})
}
export function deleteQuestion(query){
	return request({
		url:'/base/user/question/delete/question',
		method:'get',
		params:query
	})
}
export function deleteQuestionItems(query){
	return request({
		url:'/base/user/question/delete/questionItems',
		method:'get',
		params:query
	})
}
export function updateQuestion(data){
	return request({
		url:'/base/user/question/update',
		method:'post',
		data
	})
}