import request from '@/utils/request'

export function listSchool(query) {
  return request({
    url: 'http://kanghuaocean.cn:3020/mall/school/list',
    method: 'get',
    params: query
  })
}
export function AddSchool(data) {
  return request({
    url: 'http://kanghuaocean.cn:3050/school/create',
    method: 'post',
    data
  })
}
export function UpdateSchool(data) {
  return request({
    url: 'http://kanghuaocean.cn:3050/school/update',
    method: 'post',
    data
  })
}
export function DeleteSchool(query) {
  return request({
    url: 'http://kanghuaocean.cn:3050/school/delete',
    method: 'get',
    params:query
  })
}
